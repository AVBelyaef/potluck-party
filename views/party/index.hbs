<main role="main" class="row justify-content-center">
    <ul class="list-group list-group-flush w-75">
        {{#each party}}
            <li class="list-group-item">
                <a href="/party/{{_id}}" class="entry-title font-2 pad-b-1-4 c-white">{{name}}</a>
                <p class="mb-0 "> <i class="far fa-calendar-check"></i> {{startsAt}}</p>
                <p class=""> <i class="fas fa-map-marker-alt"></i> {{location}}</p>
            </li>
        {{/each}}
    </ul>
</main>

